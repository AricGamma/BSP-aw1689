<?xml version="1.0" encoding="utf-8"?>
 <Package xmlns="urn:Microsoft.WindowsPhone/PackageSchema.v8.00"
    Owner="$(OEMNAME)"
    OwnerType="OEM"
    ReleaseType="Production"
    Platform="Common"
    Component="Custom"
    SubComponent="Cmd">
    <Components>
       <OSComponent>
         <Files>
            <!--
            Source : OEM Customisation command is product specific, so copied from the Product directory
            Destination : C:\Windows\System32 which is the default. So destination not specified
            -->
             <File Source="$(PRJDIR)\Products\$(PROD)\oemcustomization.cmd" />
        </Files>
       </OSComponent>
    </Components>
 </Package>