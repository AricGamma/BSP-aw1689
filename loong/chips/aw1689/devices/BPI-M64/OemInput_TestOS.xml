<?xml version="1.0" encoding="utf-8"?>
<OEMInput xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.microsoft.com/embedded/2004/10/ImageUpdate">
  <Description>Windows 10 IoT Core Allwinner BPI-M64 test ffu</Description>
  <SOC>aw1689</SOC>
  <SV>Allwinner Technology</SV>
  <Device>BPI-M64</Device>
  <ReleaseType>Test</ReleaseType>
  <BuildType>fre</BuildType>
  <SupportedLanguages>
    <UserInterface>
      <Language>en-us</Language>
    </UserInterface>
    <Keyboard>
      <Language>en-us</Language>
    </Keyboard>
    <Speech>
      <Language>en-us</Language>
    </Speech>
  </SupportedLanguages>
  <BootUILanguage>en-us</BootUILanguage>
  <BootLocale>en-us</BootLocale>
  <Resolutions>
    <Resolution>1280x800</Resolution>
  </Resolutions>
  <AdditionalFMs>
    <AdditionalFM>%AKROOT%\FMFiles\arm\IoTUAPNonProductionPartnerShareFM.xml</AdditionalFM>
    <AdditionalFM>%WorkRoot%\loong\chips\%Chipset%\devices\default\AW1689FM.xml</AdditionalFM>
    <AdditionalFM>%WorkRoot%\loong\chips\%Chipset%\devices\%DeviceName%\DeviceFM.xml</AdditionalFM>
	<AdditionalFM>%WorkRoot%\loong\chips\%Chipset%\devices\default\OEMCommonFM.xml</AdditionalFM>
	<AdditionalFM>%WorkRoot%\src\app\AppxFM.xml</AdditionalFM>
  </AdditionalFMs>
  <Features>
    <Microsoft> 
    <!--<Feature>IOT_EFIESP</Feature>-->
    <Feature>IOT_EFIESP_TEST</Feature>
	  <Feature>IOT_EFIESP_BCD</Feature>
    <Feature>IOT_DMAP_DRIVER</Feature> 
	  <Feature>IOT_CP210x_MAKERDRIVER</Feature>
    <Feature>IOT_FTSER2K_MAKERDRIVER</Feature>
	
	  <!-- Appx installation -->
    <Feature>IOT_UAP_OOBE</Feature> 
	  <Feature>IOT_APP_TOOLKIT</Feature> 
	  <Feature>IOT_ALLJOYN_APP</Feature>
    <Feature>IOT_BERTHA</Feature>
	
  	<!-- For connectity -->
